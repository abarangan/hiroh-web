{
  "name": "hiroh-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://hiroh.io",
  "dependencies": {
    "@plausible-analytics/tracker": "^0.4.3",
    "@shopify/storefront-api-client": "^1.0.9",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "PUBLIC_URL=https://abarangan.github.io/hiroh-web react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy:staging": "npm run build:staging",
    "predeploy:production": "npm run build",
    "deploy": "npm run deploy:production",
    "deploy:production": "gh-pages -d build -r $(git remote get-url avarana)",
    "deploy:staging": "gh-pages -d build -r $(git remote get-url origin)"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12"
  }
}
